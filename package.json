{
	"name": "discord-bot-stats",
	"version": "1.0.1",
	"description": "A Node.js package to have statistics graphs for your Discord Bot",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/"
	],
	"module": "dist/index.mjs",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./src/*": "./dist/*",
		"./dist/*": "./dist/*"
	},
	"scripts": {
		"test": "tsc --noEmit",
		"build": "tsc",
		"watch": "tsc --watch",
		"lint": "eslint . --ext .ts",
		"prettier": "prettier --check **/*.ts",
		"check": "yarn lint && yarn prettier && yarn test",
		"format": "prettier --write **/*.ts",
		"typedoc": "typedoc --options typedoc.json",
		"docs": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/index.yml --output docs/docs.json",
		"testcode": "tsc && node dist/tests/test.js"
	},
	"homepage": "https://warix8.github.io/discord-bot-stats/pages/Welcome/index.html",
	"repository": "https://github.com/warix8/discord-bot-stats.git",
	"author": "Warix <39554785+warix8@users.noreply.github.com>",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@devsnowflake/docgen": "devsnowflake/docgen#ts-patch",
		"@favware/npm-deprecate": "^1.0.5",
		"@knodes/typedoc-plugin-pages": "^0.23.1",
		"@types/node": "^18.6.2",
		"@types/node-cron": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"discord.js": "^14.1.2",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-tsdoc": "^0.2.16",
		"jsdoc-babel": "^0.5.0",
		"mongoose": "^6.5.1",
		"prettier": "^2.7.1",
		"typedoc": "^0.23.9",
		"typedoc-plugin-missing-exports": "^1.0.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"chart.js": "^3.8.2",
		"chartjs-adapter-date-fns": "^2.0.0",
		"chartjs-node-canvas": "^4.1.6",
		"date-fns": "^2.29.1",
		"node-cron": "^3.0.1",
		"uuid": "^8.3.2"
	},
	"keywords": [
		"discord",
		"bot",
		"stats",
		"statistics",
		"graph",
		"chart",
		"chartjs",
		"node",
		"js",
		"javascript",
		"typescript",
		"ts",
		"discordjs",
		"discord.js",
		"discord-bot",
		"discord-stats",
		"bot-stats",
		"discord-bot-stats"
	]
}
